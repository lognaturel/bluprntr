{
  "manifest_version": 2,
  "name": "BluPrntr",
  "version": "0.1",
  "permissions": [
    "declarativeContent",
    "storage",
    "tabs",
    "<all_urls>",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  /*
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  */
  "browser_action": {
    "default_title": "BluPrntr"
  },
  "devtools_page": "devtools.html",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}
